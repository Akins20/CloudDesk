# CloudDesk License Portal Environment
# Copy to .env.local and fill in your values

# API URL (License Server)
NEXT_PUBLIC_API_URL=http://localhost:3001/api

# App Info
NEXT_PUBLIC_APP_NAME=CloudDesk
NEXT_PUBLIC_APP_URL=http://localhost:3002

# Stripe (publishable key only - safe for frontend)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_live_xxx

# CloudDesk URLs
NEXT_PUBLIC_CLOUDDESK_URL=https://clouddesk.io
NEXT_PUBLIC_DOCS_URL=https://clouddesk.io/docs
